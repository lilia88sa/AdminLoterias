    <script>
        var inicio = 1;
        //var num_pagina=[1,2]
        var resultado =0;
        var idleTime = 0;
        $(document).ready(function(){
                $("#resultados").hide();



                    var idleInterval = setInterval(timerIncrement, 10000); // 5 segundos

                    //Zero the idle timer on mouse movement.
                    $(this).mousemove(function (e) {
                        idleTime = 0;
                    });
                    $(this).keypress(function (e) {
                        idleTime = 0;
                    });


            function timerIncrement() {
                idleTime = idleTime + 1;
                if (idleTime > 1) { // 5 segundos
                    if (inicio == 1) {
                        $("#inicio").hide();

                        $("#resultados").fadeIn("slow");
                        $("#resultados").fadeIn(5000);

                        resultado=1;
                        inicio=0;
                         idleTime = 0;
                    }
                    else
                    {
                        $("#resultados").hide();
                        $("#inicio").fadeIn("slow");
                        $("#inicio").fadeIn(5000);
                        inicio =1;
                        resultado=0;
                         idleTime = 0;
                    }
                }
            }
        });


    </script>



