{% block body %}
    {% macro bote_millon(bote) %}

        {% set bote_m= bote %}
        {#{'000000'}#}

        {% if 'M' in bote and ',' in bote %}

            {% set aux=((bote|split(' ')) [0] )|replace({',':'.'})%}
            {% set val=_self.repeat(aux,'0',3-(((aux|split('.')) [1])|length)) %}

            {{val}}
        {% elseif 'M' in bote and ',' not in bote  %}
            {% set aux=(bote|split(' ')) [0] %}
            {{aux~'.000.000'}}
        {% else %}
            {{bote}}

        {% endif %}

    {% endmacro %}

    {% macro repeat(c,a,b)%}
        {% set aux='' %}
        {% for i in 1..b %}
            {% set aux=aux~a %}
        {% endfor %}
        {{c~aux~'.000'}}
    {% endmacro %}

